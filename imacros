WAIT SECONDS=5
TAG POS=1 TYPE=A ATTR=TXT:Claim

ONDOWNLOAD FOLDER=C:\dev\js_dev\node FILE=+_file23.jpg 

'TAG POS=1 TYPE=IMG ATTR=ID:adcopy-puzzle-image-image CONTENT=EVENT:SAVEITEM
TAG POS=1 TYPE=IMG ATTR=ID:adcopy-puzzle-image-image CONTENT=EVENT:SAVE_ELEMENT_SCREENSHOT 


'TAG POS=1 TYPE=INPUT:TEXT FORM=ACTION:faucet ATTR=ID:adcopy_response CONTENT=open<SP>season
'TAG POS=1 TYPE=BUTTON FORM=ACTION:faucet ATTR=ID:button

